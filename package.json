{
  "name": "@wandyezj/standard-node",
  "version": "0.0.5",
  "description": "Standard node operations in nice wrappers.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "test": "jest",
    "prettier-check": "./node_modules/.bin/prettier --check src/**/*.ts",
    "prettier": "./node_modules/.bin/prettier --config .prettierrc --write src/**/*.ts",
    "manual": "./node_modules/.bin/ts-node ./test/manual.ts",
    "clean": "if exist dist (rmdir /s /q dist)",
    "build": "./node_modules/.bin/tsc",
    "api-extractor": "./node_modules/.bin/api-extractor",
    "api-extractor-run": "./node_modules/.bin/api-extractor run",
    "api-documenter":"./node_modules/.bin/api-documenter",
    "api-documenter-run":"node_modules/.bin/api-documenter markdown --input-folder temp --output-folder documentation"

  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wandyezj/standard-node.git"
  },
  "author": "wandyezj",
  "license": "Unlicense",
  "bugs": {
    "url": "https://github.com/wandyezj/standard-node/issues"
  },
  "homepage": "https://github.com/wandyezj/standard-node#readme",
  "dependencies": {
    "@microsoft/api-documenter": "^7.7.16",
    "@types/node": "10.14.15"
  },
  "devDependencies": {
    "@microsoft/api-extractor": "^7.5.2",
    "@types/jest": "^24.0.18",
    "jest": "^24.9.0",
    "prettier": "1.18.2",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.5.2",
    "typescript": "^3.5.3"
  }
}
